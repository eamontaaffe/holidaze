{
  "name": "holidaze",
  "description": "Holidaze",
  "version": "1.0.0",
  "author": "Eamon Taaffe <eamontaaffe@gmail.com>, Florence Li Ting Fong <florenceltf@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.73",
    "gatsby-image": "^1.0.16",
    "gatsby-link": "^1.6.22",
    "gatsby-plugin-google-analytics": "^1.0.17",
    "gatsby-plugin-sharp": "^1.6.13",
    "gatsby-remark-images": "^1.5.24",
    "gatsby-source-filesystem": "^1.5.5",
    "gatsby-transformer-remark": "^1.7.18",
    "gatsby-transformer-sharp": "^1.6.13"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "UNLICENSED",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "npm run clean && gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "clean": "find . -name \".DS_Store\" -depth -exec rm {} \\;",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy:ci": "gatsby build --prefix-paths && gh-pages -d public --repo \"https://${GIT_TOKEN}@github.com/eamontaaffe/holidaze.git\""
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "prettier": "^1.7.4"
  }
}
